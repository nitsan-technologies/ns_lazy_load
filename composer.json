{
  "name": "nitsan/ns-lazy-load",
  "type": "typo3-cms-extension",
  "description": "Are you facing speed and performance issues on your TYPO3 website? If yes, then install our ns_lazy_load plug-and-play extension, which can reduce HTTPS request mechanism and improve your loading time.",
  "homepage": "https://extensions.typo3.org/extension/ns_lazy_load/",
  "license": "GPL-2.0+",
  "keywords": [
  ],
  "authors": [
    {
      "name": "NITSAN Technologies",
      "role": "Developer",
      "homepage": "https://www.nitsan.in/"
    }
  ],
  "require": {
    "typo3/cms-core": "^8.0  || ^9.0 || ^10.0  || ^11.0 "
  },
  "replace": {
    "typo3-ter/ns-lazy-load": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/public",
        "extension-key": "ns_lazy_load"
    }
  }
}
